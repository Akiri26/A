public class GenerateRealNumbers {
    public static void main(String[] args) {
        // 生成第一个实数a（-90 ≤ a ≤ 90）
        double a = -90 + Math.random() * 180;
        
        // 生成第二个实数b（-180 ≤ b ≤ 180）
        double b = -180 + Math.random() * 360;
        
        // 格式化输出，保留最多6位小数
        System.out.printf("%.6f %.6f", a, b);
    }
}
        scanner.close();
    }
}
